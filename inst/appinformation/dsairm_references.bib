  revised = {2016-10-18},
  keywords = {Biological,drug therapy,etiology,growth \& development,Humans,immunology,Leprosy,Mathematics,microbiology,Models,Mycobacterium Infections,Mycobacterium leprae,Mycobacterium tuberculosis,Time Factors,Tuberculosis},
  timestamp = {2018.05.15}
