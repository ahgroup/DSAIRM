  revised = {2016-12-03},
  keywords = {Administration,Adult,Antiviral Agents,drug therapy,Guanidines,H1N1 Subtype,Human,Humans,Influenza,Influenza A Virus,Interferons,Intranasal,isolation \& purification,Kinetics,Models,Nose,pharmacology,physiology,Pyrans,Sialic Acids,Theoretical,therapeutic use,virology,Zanamivir},
