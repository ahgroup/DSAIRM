  revised = {2018-02-17},
  keywords = {Anti-Bacterial Agents,Bacteria,Bacterial,Bacterial Infections,Cellular,Drug Administration Schedule,drug effects,Drug Resistance,drug therapy,Genetic,genetics,growth \& development,Humans,Immunity,Immunological,immunology,Microbial,microbiology,Models,Mutation,Patient Compliance,Selection,therapeutic use}
